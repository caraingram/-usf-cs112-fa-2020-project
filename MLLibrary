public class DataPoint{
  private double f1,f2;
  private String label,type;
  
  public DataPoint() { }
  
  public DataPoint(double par1, double par2, String par3, String par4){
     this.f1=par1;
     this.f2=par2;
     this.label=par3;
     this.type=par4;
  }
  //accessors
  public double getF1() { return this.f1; }
  public double getF2() { return this.f2; }
  public String getLabel() { return this.label; }
  public String getType() { return this.type; }
  
  //mutators
  public void setF1(double in) { this.f1=in; }
  public void setF2(double in) { this.f2=in; }
  public void setLabel(String in) { this.label=in; }
  public void setType(String in) { this.type=in; }
}


public abstract class Model{
  public abstract void train(ArrayList<DataPoint> data);
  public abstract String test(ArrayList<DataPoint> data);
  public abstract Double getAccuracy(ArrayList<DataPoint> data);
  public abstract Double getPrecision(ArrayList<DataPoint> data);
}

public class DummyModel extends Model{
  public abstract void train(ArrayList<DataPoint> data){ //implement function }
  public abstract String test(ArrayList<DataPoint> data){ //implement function }
  public abstract Double getAccuracy(ArrayList<DataPoint> data){ //implement function }
  public abstract Double getPrecision(ArrayList<DataPoint> data){ //implement function }
}

public class Driver{
  public Static void main(String[] args){
    data1=new DataPoint(1.0,2.0,"Red","Apple");
  }
}
}
